{
  "name": "targetym",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.18.1",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest __tests__/unit",
    "test:integration": "jest __tests__/integration",
    "test:e2e": "jest __tests__/e2e",
    "test:all": "npm run test && npm run test:integration",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "type-check": "tsc --noEmit --incremental",
    "type-check:clean": "rm -rf .tsbuildinfo && tsc --noEmit",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:reset": "npx supabase db reset",
    "supabase:push": "npx supabase db push",
    "supabase:types": "npx supabase gen types typescript --local > src/types/database.types.ts",
    "supabase:types:remote": "tsx scripts/generate-types.ts",
    "supabase:migrate": "tsx scripts/migrate-supabase.ts",
    "supabase:deploy": "npx supabase functions deploy",
    "supabase:test": "psql -h localhost -p 54322 -U postgres -d postgres -f supabase/tests/test_rls.sql",
    "oauth:setup": "node scripts/setup-oauth-wizard.js",
    "oauth:check": "node scripts/check-oauth-config.js",
    "registry:build": "tsx scripts/registry-build.ts",
    "registry:validate": "tsx scripts/registry-validate.ts",
    "registry:publish": "npm run registry:build && npm run registry:validate && tsx scripts/registry-publish.ts",
    "registry:dev": "tsx watch scripts/registry-build.ts",
    "setup": "npx tsx scripts/validate-env.ts",
    "check:auth": "npx tsx scripts/check-auth.ts",
    "check:health": "npm run type-check && npm run lint",
    "check:all": "npm run setup && npm run check:auth && npm run check:health",
    "validate:production": "tsx scripts/validate-production-env.ts",
    "clean": "rm -rf .next dist .tsbuildinfo",
    "clean:all": "npm run clean && rm -rf node_modules pnpm-lock.yaml",
    "dev:fresh": "npm run clean && npm run supabase:reset && npm run dev"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.40",
    "@ai-sdk/openai": "^2.0.59",
    "@clerk/nextjs": "^6.35.5",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sentry/nextjs": "^10.26.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.5",
    "razorpay": "^2.9.0",
    "stripe": "^12.12.0",
    "@stripe/stripe-js": "^2.0.0",
    "react-joyride": "^2.0.0",
    "ai": "^5.0.86",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.4",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "recharts": "^3.2.1",
    "sonner": "^2.0.7",
    "svix": "^1.81.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "daisyui": "^5.5.5",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "import-in-the-middle": "^2.0.0",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "msw": "^2.11.3",
    "require-in-the-middle": "^8.0.1",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
